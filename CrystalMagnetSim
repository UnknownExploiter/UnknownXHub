local function HQHHZ_fake_script() -- TabsButtons.LocalScript 
	local script = Instance.new('LocalScript', TabsButtons)

	script.Parent.Misc.MouseButton1Click:Connect(function()
		script.Parent.Parent.Tabs.Player.Visible = false
		script.Parent.Parent.Tabs.Misc.Visible = true
	end)
	
	script.Parent.Player.MouseButton1Click:Connect(function()
		script.Parent.Parent.Tabs.Player.Visible = true
		script.Parent.Parent.Tabs.Misc.Visible = false
	end)
end
